                        <table border="0" cellpadding="0" cellspacing="0" role="presentation" style width="100%">
                          <tbody>
                            <tr>
                              <td align="left" style="font-size:0px;padding:0px;word-break:break-word;">
                                <table cellpadding="0" cellspacing="0" width="100%" border="0" style="color:#000000;font-family:Poppins, Arial, sans-serif;font-size:13px;line-height:22px;table-layout:auto;width:100%;border:none;">
                                  <tr style="background-color: #ffffff">
                                    <td style="padding: 20px 0px 20px 0px">
                                      <div style="
                  text-align: center;
                  font-size: 14px;
                  font-weight: 700;
                  color: #5454a1;
                  line-height: 14px;
                "> CATÉGORIE : </div>
                                      <div style="
                  padding: 12px 0px 0px 0px;
                  text-align: center;
                  font-size: 12px;
                  font-weight: 700;
                  line-height: 14px;
                ">
                                        {{categoryType}}
                                      </div>
                                    </td>
                                  </tr>
                                  <tr style="height: 20px"></tr>
                                  <tr style="background-color: #f7f7f9">
                                    <td style="padding: 20px 20px 20px 20px">
                                      <div style="text-align: center; font-size: 14px; font-weight: 700; line-height: 14px"> DESCRIPTION : </div>
                                      <div style="
                  padding: 20px 0px 0px 0px;
                  text-align: left;
                  font-size: 14px;
                  height: 167px;
                  line-height: 16px;
                  color: #6d6d88;
                ">
                                        {{description}}
                                      </div>
                                    </td>
                                  </tr>
                                </table>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
              <!--[if mso | IE]></td></tr></table><![endif]-->
            </td>
          </tr>
