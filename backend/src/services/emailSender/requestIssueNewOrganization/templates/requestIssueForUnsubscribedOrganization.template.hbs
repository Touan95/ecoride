    <!--[if mso | IE]></td></tr></table><table align="center" border="0" cellpadding="0" cellspacing="0" class="" role="presentation" style="width:600px;" width="600" ><tr><td style="line-height:0px;font-size:0px;mso-line-height-rule:exactly;"><![endif]-->
    <div style="margin:0px auto;max-width:600px;">
      <table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" style="width:100%;">
        <tbody>
          <tr>
            <td style="direction:ltr;font-size:0px;padding:0;text-align:center;">
              <!--[if mso | IE]><table role="presentation" border="0" cellpadding="0" cellspacing="0"><tr><![endif]-->
              <!-- Colonne pour les photos -->
              <!--[if mso | IE]><td class="" style="vertical-align:top;width:300px;" ><![endif]-->
              <div class="mj-column-per-50 mj-outlook-group-fix" style="font-size:0px;text-align:left;direction:ltr;display:inline-block;vertical-align:top;width:100%;">
                <table border="0" cellpadding="0" cellspacing="0" role="presentation" width="100%">
                  <tbody>
                    <tr>
                      <td style="vertical-align:top;padding:0px 10px 0px 0px;">
                        <table border="0" cellpadding="0" cellspacing="0" role="presentation" style width="100%">
                          <tbody>
                            <tr>
                              <td align="left" class="table-height" style="min-height: 1000px; vertical-align: top; background: #FFFFFF; font-size: 0px; padding: 0px 10px 10px 10px; word-break: break-word;" valign="top">
                                <table cellpadding="0" cellspacing="0" width="100%" border="0" style="color:#000000;font-family:Poppins, Arial, sans-serif;font-size:13px;line-height:22px;table-layout:auto;width:100%;border:none;">
                                  <tr>
                                    <td colspan="2" style="
              text-align: center;
              font-size: 14px;
              font-weight: 700;
              color: #5454a1;
              line-height: 14px;
              padding: 20px 0px 20px 0px;
            "> PHOTOS : </td>
                                  </tr>
                                  <tr>
                                    {{#if firstPhotoURL}}
                                      <td style="width: 110px; height: 110px; padding: 10px 10px 10px 10px">
                                        <img src="{{firstPhotoURL}}" alt="Premiere photo" style="border: solid 1px #ececf0; width: {{#unless secondPhotoURL}}50%{{else}}100%{{/unless}}; height: 100%; object-fit: cover">
                                      </td>
                                    {{/if}}
                                    {{#if secondPhotoURL}}
                                      <td style="width: 110px; height: 110px; padding: 10px 10px 10px 10px">
                                        <img src="{{secondPhotoURL}}" alt="Deuxième photo" style="border: solid 1px #ececf0; width: 100%; height: 100%; object-fit: cover">
                                      </td>
                                    {{/if}}
                                  </tr>
                                  <tr>
                                    {{#if thirdPhotoURL}}
                                      <td style="width: 110px; height: 110px; padding: 10px 10px 10px 10px">
                                        <img src="{{thirdPhotoURL}}" alt="Troisième photo" style="border: solid 1px #ececf0; width: 100%; height: 100%; object-fit: cover">
                                      </td>
                                    {{/if}}
                                    {{#if totalPhotoNumber}}
                                      <td style="width: 110px; height: 110px; padding: 10px 10px 10px 10px">
                                        <button style="
                border: solid 1px #ececf0;
                background-color: #ffffff;
                height: 100%;
                width: 100%;
              "> + {{totalPhotoNumber}}
                                        </button>
                                      </td>
                                    {{/if}}
                                  </tr>
                                </table>
                              </td>
                            </tr>
                            {{#unless firstPhotoURL}}
                              <tr>
                                <td style="background:#FFFFFF;font-size:0px;word-break:break-word;">
                                  <div style="height:148px;line-height:148px;">&#8202;</div>
                                </td>
                              </tr>
                            {{/unless}}{{#unless thirdPhotoURL}}
                              <tr>
                                <td style="background:#FFFFFF;font-size:0px;word-break:break-word;">
                                  <div style="height:137px;line-height:137px;">&#8202;</div>
                                </td>
                              </tr>
                            {{/unless}}
                          </tbody>
                        </table>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
              <!--[if mso | IE]></td><![endif]-->
              <!-- Colonne pour catégorie et description -->
              <!--[if mso | IE]><td class="" style="vertical-align:top;width:300px;" ><![endif]-->
              <div class="mj-column-per-50 mj-outlook-group-fix" style="font-size:0px;text-align:left;direction:ltr;display:inline-block;vertical-align:top;width:100%;">
